# Environment Configuration
ENVIRONMENT=development

# API Configuration
BASE_API_URL=http://localhost:8080/api
VISITS_API_URL=http://localhost:3008/api

# AWS S3 Configuration
AWS_REGION=us-east-1
S3_BUCKET_NAME=your-nursing-app-bucket
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key

# Audio Recording Settings
MAX_RECORDING_DURATION_SECONDS=300
AUDIO_FILE_PREFIX=audio_recordings/

# Photo Settings
MAX_PHOTO_SIZE_MB=5
PHOTO_QUALITY=90
MAX_PHOTO_RESOLUTION=1080
PHOTO_FILE_PREFIX=photos/

# App Settings
APP_NAME=Nurse Mobile App
APP_VERSION=1.0.0
ENABLE_DEBUG_LOGGING=true

# Timeouts (in seconds)
API_TIMEOUT_SECONDS=30
UPLOAD_TIMEOUT_SECONDS=60